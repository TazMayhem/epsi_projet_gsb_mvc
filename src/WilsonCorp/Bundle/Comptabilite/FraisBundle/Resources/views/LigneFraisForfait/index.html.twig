{% extends 'WilsonCorpComptabiliteFraisBundle::base.html.twig' %}

{% block body -%}
    <h1>LigneFraisForfait list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idvisiteur</th>
                <th>Mois</th>
                <th>Idfraisforfait</th>
                <th>Quantite</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('lignefraisforfait_show', { 'id': entity.idVisiteur }) }}">{{ entity.idVisiteur }}</a></td>
                <td>{{ entity.mois }}</td>
                <td>{{ entity.idFraisForfait }}</td>
                <td>{{ entity.quantite }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('lignefraisforfait_show', { 'id': entity.idVisiteur }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('lignefraisforfait_edit', { 'id': entity.idVisiteur }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('lignefraisforfait_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
